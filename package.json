{
  "name": "@squareroof/ui-components",
  "version": "1.0.16",
  "description": "UI components for Squareroof ",
  "main": "src/index.ts",
  "scripts": {
    "build:vue": "rollup -c && rollup -c --environment MINIFY",
    "build:vue:watch": "rollup -c --watch",
    "build:style": "npx tailwindcss -i  ./src/styles/index.css -o  dist/library.css  && cleancss -o dist/library.min.css dist/library.css",
    "build:style:watch": "npx tailwindcss -i  ./src/styles/index.css -o  dist/library.min.css --watch",
    "build:lib": "rimraf dist && npm run build:vue && npm run build:style",
    "publish:lib": "npm run build:lib && npm publish",
    "watch": "npm run build:vue:watch & npm run build:style:watch"
  },
  "peerDependencies": {
    "vue": "^3.2.21",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^14.0.0",
    "@tailwindcss/line-clamp": "^0.3.0",
    "@tailwindcss/postcss7-compat": "^2.2.17",
    "@tailwindcss/typography": "^0.5.0",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.178",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^2.0.0-rc.16",
    "@vue/vue3-jest": "^27.0.0-alpha.3",
    "babel-jest": "^27.3.1",
    "clean-css-cli": "^5.6.1",
    "cypress": "^8.7.0",
    "eslint": "^8.4.1",
    "eslint-plugin-vue": "^8.2.0",
    "jest": "^27.3.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.33.0",
    "rollup-plugin-vue": "^6.0.0",
    "sass-loader": "^13.0.2",
    "tailwindcss": "^3.1.8",
    "ts-jest": "^27.0.7",
    "typescript": "^4.3.5"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^9.8.8",
    "lottie-web": "^5.9.6",
    "openplayerjs": "^2.13.0",
    "postcss": "^7.0.39",
    "rollup-plugin-css-only": "^3.1.0",
    "sass": "^1.54.9",
    "swiper": "^8.4.2"
  }
}
