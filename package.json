{
  "name": "app-ui-components",
  "version": "0.0.1",
  "description": "UI components for {App name}",
  "author": "",
  "scripts": {
    "serve": "webpack serve",
    "build": "webpack --mode=production --node-env=production && npm run build:style",
    "build:dev": "webpack --mode=development",
    "build:lib": "rimraf dist && npm run build:vue && npm run build:style",
    "build:prod": "webpack --mode=production --node-env=production && npm run build:style",
    "build:style": "npx tailwindcss -i  ./src/styles/index.css -o  dist/library.css  && cleancss -o dist/library.min.css dist/library.css",
    "build:style:watch": "npx tailwindcss -i  ./src/styles/index.css -o  dist/library.min.css --watch",
    "publish:lib": "npm run build:lib && npm publish",
    "watch": "webpack --watch"
  },
  "main": "src/index.ts",
  "dependencies": {
    "@vue/compiler-sfc": "^3.2.45",
    "autoprefixer": "^10.4.14",
    "moment": "^2.29.4",
    "postcss": "^8.4.23",
    "sass": "^1.54.9",
    "style-loader": "^3.3.1",
    "swiper": "^8.4.2",
    "tailwindcss": "^3.3.2",
    "vue": "^3.2.21",
    "vue-loader": "^17.0.1"
  },
  "devDependencies": {
    "@tailwindcss/line-clamp": "^0.3.1",
    "@tailwindcss/typography": "^0.5.0",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.178",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^2.0.0-rc.16",
    "@vue/vue3-jest": "^27.0.0-alpha.3",
    "babel-jest": "^27.3.1",
    "clean-css-cli": "^5.6.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "cypress": "^8.7.0",
    "declaration-bundler-webpack-plugin": "^1.0.3",
    "eslint": "^8.4.1",
    "eslint-plugin-vue": "^8.2.0",
    "jest": "^27.3.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^13.0.2",
    "tailwind-scrollbar": "^3.0.0",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.4.2",
    "typescript": "^4.3.5",
    "vue-cli-plugin-tailwind": "~3.0.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "wepack-cli": "^0.0.1-security"
  },
  "peerDependencies": {
    "vue": "^3.2.21",
    "vue-router": "^4.0.12"
  },
  "license": "ISC"
}
